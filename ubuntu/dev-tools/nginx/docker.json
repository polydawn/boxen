{
	"Command": [
		"/service/bin/nginx",  "-g", "daemon off;", "-c", "/service/conf/nginx.conf"
	],
	"Build": [
		"/cwd/build.sh"
	],
	"Mounts": [
		["./conf", "/service/conf", "ro"],
		["./data", "/service/data", "ro"],
		["./log",  "/service/log",  "rw"]
	],
	"Ports": [
		["80", "80"],
		["443", "443"]
	],
	"Image":    "pd-nginx:1.2.6-dev",
	"Upstream": "pd-tools:12.10",
	"Attach":   true,
	"Purge":    true
}
